set basepath to "~/code/django-audiokitt/"set envpath to "~/srv/django-audiokitt/"set server_port to 8008tell application "iTerm"	make new terminal	tell the current terminal		activate current session		launch session "django"		tell the last session			write text "cd " & basepath & "app/"			write text "source " & envpath & "bin/activate"			write text "./manage.py runserver 0.0.0.0:" & server_port		end tell	end tell		tell i term application "System Events" to keystroke "d" using command down	tell the current terminal		tell the last session			write text "cd " & basepath			write text "gulp"		end tell	end tell		tell i term application "System Events" to keystroke "D" using command down	tell the current terminal		tell the last session			write text "cd " & basepath & "app/"			write text "source " & envpath & "bin/activate"			write text "./manage.py rqworker default"		end tell	end tell	tell i term application "System Events" to keystroke "D" using command down	tell the current terminal		tell the last session			write text "cd " & basepath & "app/"			write text "source " & envpath & "bin/activate"		end tell	end tellend tell